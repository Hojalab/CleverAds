<h1>Authentications</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>User</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @sessions.each do |session| %>
      <tr>
        <td><%= session.id %></td>
        <td><%= link_to session.user_id, session_path(session) %></td>
        <td><%= session.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_session_path(session), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', session_path(session), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_session_path, :class => 'btn btn-primary' %>
